{
  "version": 2,
  "builds": [
    {
      "src": "mysite/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mysite/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "mysite.settings"
  },
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "mysite.settings"
    }
  },
  "functions": {
    "mysite/wsgi.py": {
      "maxDuration": 30
    }
  }
}